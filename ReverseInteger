import java.io.*;

import java.util.*;

import java.text.*;

import java.math.*;

import java.util.regex.*;

public class Solution {

    public static void main(String[] args) {

       int x,n=0,m=0;

        Scanner sc=new Scanner(System.in);

        x=sc.nextInt();



       while(x!=0)

           {

            n=x%10;

            m=m*10+n;

           x=x/10;

}

        System.out.print(m);

           }

        }

    
